/* pages/fd/space/add-1.wxss */
.space-title{
  padding: 84rpx 32rpx;
  background: #ff5555;
  font-size: 48rpx;
  color: #fff;
  line-height: 68rpx;
}
.space-main{
  padding: 80rpx 32rpx 0;
}
.space-main .title{
  font-size: 40rpx;
  color: #212121;
  line-height: 56rpx;
}
.list{
  margin-top: 40rpx;
}
.item{
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.left{
  width: 500rpx;
  flex-shrink: 0;
}
.p1{
  font-size: 28rpx;
  color: #212121;
  line-height: 40rpx;
}
.p2{
  font-size: 24rpx;
  color: #9e9e9e;
  line-height: 36rpx;
  margin-top: 10rpx;
}
.right{
  width: 40rpx;
  height: 40rpx;
  flex-shrink: 0;
  background: url(data:image/png;base64,) center no-repeat;
  background-size: 100%;
}
.item.active .right{
  background-image: url(data:image/png;base64,)
}
.next{
  width: calc(100% - 64rpx);
  line-height: 46rpx;
  font-size: 32rpx;
  color: #fff;
  padding: 22rpx 0;
  border-radius: 8rpx;
  background: #ff5555;
  text-align: center;
  position: fixed;
  bottom: 32rpx;
  left: 32rpx;
}
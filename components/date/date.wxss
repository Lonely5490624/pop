/* pages/fd/calendar/index.wxss */
.calendar-title{
  font-size: 48rpx;
  color: #212121;
  padding: 48rpx 32rpx;
}
.space-list{
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  white-space: nowrap;
}
.space-item{
  width: 170rpx;
  display: inline-block;
  margin-right: 19rpx;
}
.space-item .image{
  width: 170rpx;
  height: 113rpx;
  opacity: .6;
}
.space-item .info{
  font-size: 18rpx;
  color: #9e9e9e;
  white-space: pre-wrap;
  margin-top: 12rpx;
}
.space-item.active .image{
  opacity: 1;
}
.space-item.active .info{
  color: #212121;
}

.calendar-main{
  height: calc(100% - 362rpx);
  position: absolute;
  bottom: 0;
  left: 0;
}
.month-block{
  padding: 42rpx 0 24rpx;
  border-bottom: 1rpx solid #e0e0e0;
}
.month-title{
  text-align: center;
  font-size: 24rpx;
  color: #9e9e9e;
  line-height: 34rpx;
}
.week-title{
  display: flex;
  font-size: 24rpx;
  color: #212121;
  margin-top: 34rpx;
}
.week-item{
  width: 14.28%;
  text-align: center;
}
.month-content{
  display: flex;
  flex-wrap: wrap;
  font-size: 20rpx;
  color: #616161;
  margin-top: 36rpx;
}
.month-day{
  width: 14.28%;
  text-align: center;
  vertical-align: middle;
  margin-top: 6rpx;
}
.month-day .day-item{
  width: 60rpx;
  display: inline-block;
  border-radius: 50%;
  line-height: 60rpx;
}
.month-day.startActive .day-item{
  background: #ff5555;
  color: #fff;
}
.month-day.midActive .day-item{
  background: #ff9999;
  color: #fff;
}
.month-day.endActive .day-item{
  background: #ff5555;
  color: #fff;
}
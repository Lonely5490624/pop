/* components/tuiding/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #fff;
  position: relative;
}
page .header{
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
  font-size: 28rpx;
  border-bottom: 2rpx solid #f3f3f3;
  color: #3c3c3c;
  position: relative;
}
page .header image{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 40rpx;
  right: 38rpx;
}
page .content{
  height: calc(100% - 94rpx);
  color: #9e9e9e;
  line-height: 40rpx;
  font-size: 24rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
page .content .biao{
  width: 100%;
  overflow: auto;
}
page .content .biao image{
  width: 1000rpx;
}